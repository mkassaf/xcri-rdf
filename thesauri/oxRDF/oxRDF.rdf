               <skos:Concept rdf:about="https://data.ox.ac.uk/id/ox-rdf/domain/study-and-research-skills">
                  <skos:prefLabel>Study and Research skills</skos:prefLabel>
               </skos:Concept>
            </skos:narrower>
            <skos:narrower>
               <skos:Concept rdf:about="https://data.ox.ac.uk/id/ox-rdf/domain/career-and-personal-development">
                  <skos:prefLabel>Career and Personal Development</skos:prefLabel>
               </skos:Concept>
            </skos:narrower>
            <skos:narrower>
               <skos:Concept rdf:about="https://data.ox.ac.uk/id/ox-rdf/domain/teaching-and-academic-skills">
                  <skos:prefLabel>Teaching and academic skills</skos:prefLabel>
               </skos:Concept>
            </skos:narrower>
            <skos:narrower>
               <skos:Concept rdf:about="https://data.ox.ac.uk/id/ox-rdf/domain/communication">
                  <skos:prefLabel>Communication</skos:prefLabel>
               </skos:Concept>
            </skos:narrower>
         </skos:Concept>
      </skos:hasTopConcept>
