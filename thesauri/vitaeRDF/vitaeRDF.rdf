                  <skos:narrower>
                     <skos:Concept rdf:about="http://id.vitae.ac.uk/rdf/descriptor/A.1.2">
                        <skos:prefLabel>Research methods – theoretical knowledge</skos:prefLabel>
                        <skos:notation rdf:datatype="http://id.vitae.ac.uk/rdf/notation">A.1.2</skos:notation>
                        <skos:narrower>
                           <skos:Concept rdf:about="http://id.vitae.ac.uk/rdf/descriptor/A.1.2.1">
                              <skos:prefLabel>Phase 1 of Research methods – theoretical knowledge</skos:prefLabel>
                              <skos:notation rdf:datatype="http://id.vitae.ac.uk/rdf/notation">A.1.2.1</skos:notation>
                              <skos:definition>Understands relevant research methodologies and techniques and their appropriate application within own research area. (A4)*

Justifies the principles and experimental techniques used in own research. (B6)*</skos:definition>
                           </skos:Concept>
                        </skos:narrower>
                        <skos:narrower>
                           <skos:Concept rdf:about="http://id.vitae.ac.uk/rdf/descriptor/A.1.2.2">
                              <skos:prefLabel>Phase 2 of Research methods – theoretical knowledge</skos:prefLabel>
                              <skos:notation rdf:datatype="http://id.vitae.ac.uk/rdf/notation">A.1.2.2</skos:notation>
                              <skos:definition>Appreciates the value of a range of standards and methods/techniques for information/data collection and analysis; assesses and demonstrates usefulness and validity of information/data in the context of a specific problem/question.</skos:definition>
                           </skos:Concept>
                        </skos:narrower>
                        <skos:narrower>
                           <skos:Concept rdf:about="http://id.vitae.ac.uk/rdf/descriptor/A.1.2.3">
                              <skos:prefLabel>Phase 3 of Research methods – theoretical knowledge</skos:prefLabel>
                              <skos:notation rdf:datatype="http://id.vitae.ac.uk/rdf/notation">A.1.2.3</skos:notation>
                              <skos:definition>Combines and justifies methods/techniques designed specifically for an investigation in a flexible and vigorous manner.</skos:definition>
                           </skos:Concept>
                        </skos:narrower>
                        <skos:narrower>
                           <skos:Concept rdf:about="http://id.vitae.ac.uk/rdf/descriptor/A.1.2.4">
                              <skos:prefLabel>Phases 4–5 of Research methods – theoretical knowledge</skos:prefLabel>
                              <skos:notation rdf:datatype="http://id.vitae.ac.uk/rdf/notation">A.1.2.4</skos:notation>
                              <skos:definition>Recognises the value of alternative research paradigms and is able to work in, and support others working in, an inter-disciplinary way.</skos:definition>
                           </skos:Concept>
                        </skos:narrower>
