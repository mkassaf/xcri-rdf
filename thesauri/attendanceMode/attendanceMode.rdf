<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">
   <skos:ConceptScheme rdf:about="http://xcri.org/profiles/catalog/1.2/attendanceMode">
      <skos:prefLabel>Attendance Mode for Course Data Programme.  Created by Jennifer Paull, APS Ltd.  This VDEX version 5 March 2012.</skos:prefLabel>
      <skos:hasTopConcept>
         <skos:Concept rdf:about="http://xcri.org/profiles/catalog/1.2/attendanceMode/CM">
            <skos:prefLabel>Campus</skos:prefLabel>
            <skos:definition>Learner will be expected to attend at the venue for the bulk of their teaching time.</skos:definition>
            <skos:inScheme rdf:resource="http://xcri.org/profiles/catalog/1.2/attendanceMode"/>
            <skos:notation rdf:datatype="http://xcri.org/profiles/catalog/1.2/attendanceMode/notation">CM</skos:notation>
         </skos:Concept>
      </skos:hasTopConcept>
      <skos:hasTopConcept>
         <skos:Concept rdf:about="http://xcri.org/profiles/catalog/1.2/attendanceMode/DA">
            <skos:prefLabel>Distance with attendance</skos:prefLabel>
            <skos:definition>Primarily distance learning however the learner will be required to attend at the venue as necessary.</skos:definition>
            <skos:inScheme rdf:resource="http://xcri.org/profiles/catalog/1.2/attendanceMode"/>
            <skos:notation rdf:datatype="http://xcri.org/profiles/catalog/1.2/attendanceMode/notation">DA</skos:notation>
         </skos:Concept>
      </skos:hasTopConcept>
      <skos:hasTopConcept>
         <skos:Concept rdf:about="http://xcri.org/profiles/catalog/1.2/attendanceMode/DS">
            <skos:prefLabel>Distance without attendance</skos:prefLabel>
            <skos:definition>The learning opportunity does not require any attendance at the venue.</skos:definition>
            <skos:inScheme rdf:resource="http://xcri.org/profiles/catalog/1.2/attendanceMode"/>
            <skos:notation rdf:datatype="http://xcri.org/profiles/catalog/1.2/attendanceMode/notation">DS</skos:notation>
         </skos:Concept>
      </skos:hasTopConcept>
      <skos:hasTopConcept>
         <skos:Concept rdf:about="http://xcri.org/profiles/catalog/1.2/attendanceMode/MM">
            <skos:prefLabel>Mixed mode</skos:prefLabel>
            <skos:definition/>
            <skos:inScheme rdf:resource="http://xcri.org/profiles/catalog/1.2/attendanceMode"/>
            <skos:notation rdf:datatype="http://xcri.org/profiles/catalog/1.2/attendanceMode/notation">MM</skos:notation>
         </skos:Concept>
      </skos:hasTopConcept>
      <skos:hasTopConcept>
         <skos:Concept rdf:about="http://xcri.org/profiles/catalog/1.2/attendanceMode/NC">
            <skos:prefLabel>Face-to-face non-campus</skos:prefLabel>
            <skos:definition>The learner undertakes the course off campus.</skos:definition>
            <skos:inScheme rdf:resource="http://xcri.org/profiles/catalog/1.2/attendanceMode"/>
            <skos:notation rdf:datatype="http://xcri.org/profiles/catalog/1.2/attendanceMode/notation">NC</skos:notation>
         </skos:Concept>
      </skos:hasTopConcept>
      <skos:hasTopConcept>
         <skos:Concept rdf:about="http://xcri.org/profiles/catalog/1.2/attendanceMode/ON">
            <skos:prefLabel>Online (no attendance)</skos:prefLabel>
            <skos:definition>The learner undertakes the course over the internet only.</skos:definition>
            <skos:inScheme rdf:resource="http://xcri.org/profiles/catalog/1.2/attendanceMode"/>
            <skos:notation rdf:datatype="http://xcri.org/profiles/catalog/1.2/attendanceMode/notation">ON</skos:notation>
         </skos:Concept>
      </skos:hasTopConcept>
      <skos:hasTopConcept>
         <skos:Concept rdf:about="http://xcri.org/profiles/catalog/1.2/attendanceMode/WB">
            <skos:prefLabel>Work-based</skos:prefLabel>
            <skos:definition>The learner undertakes the course at their workplace.</skos:definition>
            <skos:inScheme rdf:resource="http://xcri.org/profiles/catalog/1.2/attendanceMode"/>
            <skos:notation rdf:datatype="http://xcri.org/profiles/catalog/1.2/attendanceMode/notation">WB</skos:notation>
         </skos:Concept>
      </skos:hasTopConcept>
   </skos:ConceptScheme>
</rdf:RDF>